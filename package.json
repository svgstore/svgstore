{
  "name": "svgstore",
  "version": "1.1.0",
  "description": "Combines mulitple svg files into one.",
  "main": "src/svgstore.js",
  "bin": {
    "svgstore": "./bin/svgstore"
  },
  "scripts": {
    "coveralls": "nyc report -r text-lcov | coveralls",
    "test": "xo src/*.js test/*.js; nyc ava -v",
    "posttest": "nyc report -r lcov",
    "watch": "chokidar '{src,test}/**/*.js' -c 'npm test'"
  },
  "repository": "shannonmoeller/svgstore",
  "keywords": [
    "concat",
    "join",
    "merge",
    "pack",
    "path",
    "sprite",
    "svg",
    "svgs",
    "svgstore",
    "symbol",
    "symbols"
  ],
  "author": "Shannon Moeller <me@shannonmoeller.com> (http://shannonmoeller.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shannonmoeller/svgstore/issues"
  },
  "homepage": "https://github.com/shannonmoeller/svgstore",
  "dependencies": {
    "cheerio": "^0.20.0",
    "glob": "^7.0.5",
    "object-assign": "^4.1.0"
  },
  "devDependencies": {
    "ava": "0.15.2",
    "chokidar-cli": "^1.2.0",
    "coveralls": "^2.11.6",
    "nyc": "^7.0.0",
    "xo": "^0.16"
  },
  "engines": {
    "node": ">= 0.12"
  }
}
